{
  "name": "htmlmodule",
  "version": "0.6.2",
  "description": "HTML DOM assembler library",
  "homepage": "https://aristov.github.io/htmlmodule",
  "bugs": "https://github.com/aristov/htmlmodule/issues",
  "license": "MIT",
  "author": "Vyacheslav Aristov <vv.aristov@gmail.ru> (http://twitter.com/aristov7)",
  "files": [
    "lib"
  ],
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aristov/htmlmodule.git"
  },
  "scripts": {
    "lint": "eslint lib --ignore-pattern *.spec.js --quiet && eslint lib/*.spec.js --env browser,mocha && eslint shim docs",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "mocha": "mocha",
    "karma": "karma start",
    "test-full": "npm run karma && npm test",
    "build": "NODE_ENV=production webpack",
    "watch": "webpack -w",
    "publish": "git push && git push --tags",
    "api": "rm -rf dist/api && npm run jsdoc && npm run esdoc",
    "esdoc": "esdoc",
    "jsdoc": "jsdoc lib/* -d dist/api/jsdoc --verbose",
    "gh-pages": "git branch -D gh-pages && git checkout -b gh-pages && npm run build && npm test && git add dist coverage docs/build -f && git commit -am 'Add files' && git push --set-upstream origin gh-pages -f && git checkout master && npm run build && git status"
  },
  "dependencies": {
    "dommodule": "github:aristov/dommodule"
  },
